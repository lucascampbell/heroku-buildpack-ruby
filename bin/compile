#!/usr/bin/env ruby

$:.unshift File.expand_path("../../lib", __FILE__)
require "language_pack"
puts "looking for pack"
if pack = LanguagePack.detect(ARGV[0], ARGV[1])
  puts "pack found 00 #{pack}"
  pack.log("compile") do
    puts "running compile"
    pack.compile
  end
else
  puts 'pack not found'
end